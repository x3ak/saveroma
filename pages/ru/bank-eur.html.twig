{% extends 'bank.html.twig' %}

{% block master_header %}
    <h1>EUR счет</h1>
{% endblock %}

{% block page_content %}
    <ul class="bank-details__list">
        <li><b>Получатель:</b> {{ banks.eur.name }}</li>
        <li><b>Страна:</b> {{ banks.eur.country }}</li>
        <li><b>Город:</b> {{ banks.eur.city }}</li>
        <li><b>Банк:</b> {{ banks.eur.bank }}</li>
        <li><b>Банк корреспондент:</b> {{ banks.eur.correspondent_bank }}</li>
        <li><b>IBAN:</b> {{ banks.eur.iban }}</li>
        <li><b>BIC:</b> {{ banks.eur.bic }}</li>
    </ul>
{% endblock %}
