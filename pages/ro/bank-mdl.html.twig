{% extends 'bank.html.twig' %}

{% block master_header %}
    <h1>Cont MDL</h1>
{% endblock %}

{% block page_content %}
    <ul class="bank-details__list">
        <li><b>Beneficiar:</b> {{ banks.mdl.name }}</li>
        <li><b>Țară:</b> {{ banks.mdl.country }}</li>
        <li><b>Oraș:</b> {{ banks.mdl.city }}</li>
        <li><b>Banca:</b> {{ banks.mdl.bank }}</li>
        <li><b>IBAN:</b> {{ banks.mdl.iban }}</li>
        <li><b>BIC:</b> {{ banks.mdl.bic }}</li>
        <li><b>idno:</b> {{ banks.mdl.idno }}</li>
    </ul>
{% endblock %}
